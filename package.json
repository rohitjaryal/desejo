{
  "name": "desejo",
  "version": "1.0.0",
  "scripts": {
    "start": "tsc src/index.ts && node src/index.js",
    "size": "echo $(gzip -c index.min.js | wc -c) bytes",
    "build": "esbuild src/index.js --bundle --minify --sourcemap --outfile=index.min.js --format=esm"
  },
  "keywords": [
    "fetch",
    "axios",
    "http",
    "https",
    "async",
    "ajax"
  ],
  "main": "index.min.js",
  "files": [
    "index.min.js"
  ],
  "types": "index.d.ts",
  "type": "module",
  "devDependencies": {
    "@tsconfig/node-lts": "^18.12.5",
    "@types/node": "^20.7.0",
    "esbuild": "0.19.3",
    "typescript": "^5.2.2",
    "prettier": "^3.0.3"
  }
}
